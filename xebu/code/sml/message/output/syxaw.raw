namespace sx = "http://www.hiit.fi/fuego/fc/syxaw"

Grammar
start = tree
id = attribute id { text }
dir-content = (file* & directory*)
file-content = (attribute name { text }, attribute version { text }, attribute object { text })
file = element sx:file {
(id, file-content)
}

directory = element sx:directory {
(id, file-content, dir-content)
}

tree = element sx:tree {
(id, attribute nextid { text }, attribute lid { text }, dir-content)
}


